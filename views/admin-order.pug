h1 Admin Panel
h2 Order-information
table
    tr
        th id
        th date
        th userId
        th goodsId
        th goodg cost
        th goods amount
        th total
        th user
        th phone
        th address

    each val, index in order
        tr 
            td #{val['id']}
            td #{val['human_date']}
            td #{val['user_id']}
            td #{val['goods_id']}
            td #{val['goods_cost'].toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$& ')} uah
            td #{val['goods_amount']}
            td #{val['total'].toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$& ')} uah    
            td #{val['user']}
            td #{val['phone']}
            td #{val['address']}